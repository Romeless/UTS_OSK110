( function($) {
	if (typeof $ === 'undefined') {
		return;
	}

	$( function() {
		var timeout = null;

		// Activate the map only if the cursor stays on the map for a half second.
		$( '.contact-map-container')
			.on( 'mouseenter', function() {
				var $distractor = $( this ).find( '> .contact-map-distractor' );

				clearTimeout(timeout);
				timeout = setTimeout( function() {
					$distractor.hide();
				}, 500 );
			} )
			.on( 'mouseleave', function() {
				clearTimeout(timeout);
				$( this ).find( '> .contact-map-distractor' ).show();
			} );
	} );
} )( window.jQuery );
